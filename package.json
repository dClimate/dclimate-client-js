{
  "name": "dclimate-client-js",
  "version": "0.1.0",
  "description": "JavaScript client for dClimate datasets using jaxray and IPFS sharded stores",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/browser/index.js",
      "require": "./dist/node/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build:browser": "tsc -p tsconfig.browser.json",
    "build:node": "tsc -p tsconfig.node.json",
    "build": "npm run build:browser && npm run build:node",
    "test": "vitest"
  },
  "keywords": [
    "dclimate",
    "zarr",
    "ipfs",
    "jaxray",
    "climate-data"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@dclimate/jaxray": "^0.4.0"
  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "vitest": "^3.2.4"
  }
}
